{
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "react-jsxdev",
    "jsxImportSource": "vue",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"]
  },
  "include": ["cypress/**/*", "src/vite-env.d.ts"],
  "exclude": ["node_modules"]
}
